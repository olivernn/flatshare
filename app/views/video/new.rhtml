<% form_for(:video, :url => {:controller => 'video', :action => 'upload'}, :html => {:multipart => true}) do |form| %>
	<fieldset id="add_picture">
		<legend>Add Video</legend>
		<label for="image_uploaded_data">File</label>
		<%= form.file_field "upload_file", :class => "medium_field"%><br/>
		<%= submit_tag("Upload file")%>
	</fieldset>
<% end %>